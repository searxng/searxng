{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@searxng/theme-simple",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:icons && npm run build:vite",
    "build:icons": "node theme_icons.ts",
    "build:vite": "vite build",
    "clean": "rm -Rf node_modules",
    "fix": "npm run fix:stylelint && npm run fix:biome && npm run fix:package",
    "fix:biome": "biome check --write",
    "fix:package": "sort-package-json --quiet",
    "fix:stylelint": "stylelint --fix strict 'src/**/*.{scss,sass,less,styl}'",
    "lint": "npm run lint:biome && npm run lint:tsc",
    "lint:biome": "biome lint",
    "lint:tsc": "tsc --noEmit"
  },
  "browserslist": [
    "baseline 2022",
    "not dead"
  ],
  "dependencies": {
    "ionicons": "^8.0.13",
    "normalize.css": "8.0.1",
    "ol": "^10.7.0",
    "swiped-events": "1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/node": "^25.2.1",
    "browserslist": "^4.28.1",
    "browserslist-to-esbuild": "^2.1.1",
    "edge.js": "^6.4.0",
    "less": "^4.5.1",
    "mathjs": "^15.1.0",
    "sharp": "~0.34.5",
    "sort-package-json": "^3.6.1",
    "stylelint": "^16.26.0",
    "stylelint-config-standard-less": "^4.0.1",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^4.0.0",
    "typescript": "~5.9.3",
    "vite": "8.0.0-beta.14",
    "vite-bundle-analyzer": "^1.3.6"
  }
}
